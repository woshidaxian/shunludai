<!--pages/receiptOrder/receiptOrder.wxml-->
<view class='container'>
  <view class='box' wx:for="{{orderData}}" wx:key="id">
    <view class='box-1'>
      <view class='box-1-img'>
        <image src='{{item.imgSrc}}' class='img'></image>
      </view>
    </view>
    <view class='box-2'>
      <view class='name'>{{item.title}}</view>
      <view class='time'>{{item.day+ ' ' +item.time}}</view>
    </view>
    <view class='box-3'>
      <view class='status'>{{item.status}}</view>
    </view>
  </view>
</view>
